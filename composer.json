{
    "name": "damoyo/api",
    "autoload": {
        "psr-4": {
            "Damoyo\\Api\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "clue/framework-x": "^0.17",
        "react/mysql": "^0.6.0",
        "php-di/php-di": "^7.0",
        "react/event-loop": "^1.2",
        "symfony/validator": "^7.2",
        "symfony/property-access": "^7.2",
        "symfony/property-info": "^7.2",
        "symfony/serializer": "^7.2",
        "monolog/monolog": "^3.8",
        "sentry/sentry": "^3.22",
        "php-http/react-adapter": "^4.0",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
