{
    "require": {
        "php": ">=8.3",
        "clue/framework-x": "^0.16.0",
        "react/mysql": "^0.6.0",
        "league/container": "^4.2",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "league/flysystem": "3.0"
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/",
            "App\\": "App/"
        }
    },
    "bin": [
        "main.php",
        "envs"
    ],
    "scripts": {
        "build" : "php -d phar.readonly=off ./vendor/clue/phar-composer/bin/phar-composer build . ",
        "build2" : "php -d phar.readonly=off ./build.php "
    },
    "extra": {
        "phar-composer": {
            "directories": [
                "env/local"
            ],
            "files": [
                "env/local/.env"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "wyrihaximus/composer-update-bin-autoload-path": true
        }
    }
}
